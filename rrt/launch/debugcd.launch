<launch>
	<!-- Initialize parameters -->
	<include file="$(find rrt)/launch/parameters.launch" 
	/>
	<node
		pkg="pcl_converter"
		type="pcl_converter_node"
		name="pcl_converter_node"
		required="true"
		output="screen"
		launch-prefix="xterm -e gdb -ex run --args"
	/>
	<node
		pkg="rrt"
		type="rrt_node"
		name="rrt_node"
		respawn="false"
		output="screen"
		required="true"
	/>
    <node
    	pkg="rviz"
		type="rviz"
		name="rviz"
		respawn="true"
    />
</launch>
