<launch>
	<!-- Initialize parameters -->
	<include file="$(find rrt)/launch/parameters.launch" 
	/>
	<node
		pkg="pcl_converter"
		type="pcl_converter_node"
		name="pcl_converter_node"
		respawn="false"
		required="true"
        output="screen"
	/>
	<node
		pkg="rrt"
		type="rrt_node"
		name="rrt_node"
		respawn="false"
		launch-prefix="stterm -g 100x150 -e gdb -ex run --args"
		required="true"
	/>
</launch>
